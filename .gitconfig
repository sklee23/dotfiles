[user]
	name = Sookwan Lee
[alias]
	co = checkout
	ci = commit
	br = branch
	st = status
	df = diff
	dt = difftool
	lp = log -p
	lpr = log -p --reverse
	cob = checkout -b
	hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
	type = cat-file -t
	dump = cat-file -p
	last = log -1 HEAD
[diff]
	tool = meld
[color]
	ui = auto
[core]
#	pager = less -FRSX
	editor = vim
[push]
	default = simple
